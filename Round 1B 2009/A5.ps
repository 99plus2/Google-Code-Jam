%!PS
%
% Problem: Decision Tree
% Language: PostScript
% Author: KirarinSnow
% Usage: gs -q -dNODISPLAY -dNOPROMPT thisfile.ps < input.in > output.out


/readtoken
{
    token pop
} def

/fileread
{
    (%stdin) (r) file readtoken
} def

/advance
{
    exch dup <> ne
    {
	readtoken features exch known not
	{
	    readtoken pop
	} if

	readtoken readtoken
	3 index mul

	advance
    } if
} def

/compute
{
    fileread
    /tree fileread def

    fileread
    {
	fileread
	/features
	<<
	    fileread
	    {
		fileread /
	    }
	    repeat
	>> def

	tree readtoken
	advance
	pop ==
    } repeat
} def

1 1 fileread
{
    (Case #) print
    5 string cvs print
    (: ) =

    compute
} for
