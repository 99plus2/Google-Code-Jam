cases = read()

define compute ()
{
    w = read()
    b = read()
    if (b % 2 == 1) print "BLACK" else print "WHITE"
}

for (i = 1; i <= cases; i++)
{
    print "Case #", i, ": "
    x = compute()
    print "\n"
}
