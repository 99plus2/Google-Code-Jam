#import <stdio.h>
 
void main() {
  int i, t;
  scanf("%d", &t);
  for(i=1; i<=t; i++) {
    int b, w;
    scanf("%d %d", &w, &b);
    printf("Case #%d: %s\n", i, (b%2 == 1) ? "BLACK" : "WHITE");
  }
}
