%!PS
%
% Problem: Welcome to Code Jam
% Language: PostScript
% Author: KirarinSnow
% Usage: gs -q -dNODISPLAY -dNOPROMPT thisfile.ps <input.in >output.out


/rd {(%stdin) (r) file token pop} def

/getline {(%stdin) (r) file 500 string readline pop} def


/wc [ (welcome to code jam) { } forall ] def

/compute
{
    /ct [ 20 { 0 } repeat ] def

    ct 0 1 put

    /test getline def

    test
    {
	/c exch def
	1 1 19
	{
	    /i exch def
	    c wc i 1 sub get eq
	    {
		ct i
		    ct i get
		    ct i 1 sub get
		    add
		    10000 mod
		put
	    } if
	} for
    } forall

    ct 19 get
    dup 1000 idiv 0 eq
    {
	(0) print
    } if
    dup 100 idiv 0 eq
    {
	(0) print
    } if
    dup 10 idiv 0 eq
    {
	(0) print
    } if
} def


1 1 rd
{
    (Case #) print 5 string cvs print (: ) print
    compute =
} for

quit
